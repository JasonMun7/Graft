{
  "manifest_version": 3,
  "name": "Visor",
  "version": "0.0.1",
  "action": { "default_popup": "index.html" }, 
  "permissions": [],
  "host_permissions": ["http://localhost:3000/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' http://localhost:3000"
  }
}
